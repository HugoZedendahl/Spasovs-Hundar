art = input("Artikel: ")
ant = input("Hur mÃ¥nga? ")
artList = []
artAnt = []

while art != "sluta":
    artList.append(art)
    artAnt.append(ant)
    
    art = input("Artikel: ")
    if art != "sluta":
        ant = input("Hur mÃ¥nga? ")

count = 0
for x in artList:
    print(artList[count], ":", artAnt[count], "stycken")
    count += 1
